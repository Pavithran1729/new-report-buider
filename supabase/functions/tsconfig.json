{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "noImplicitAny": false,
    "typeRoots": [
      "../../types",
      "../../node_modules/@types"
    ]
  },
  "include": [
    "**/*.ts",
    "../**/*.ts",
    "../../types/**/*.d.ts"
  ]
}
